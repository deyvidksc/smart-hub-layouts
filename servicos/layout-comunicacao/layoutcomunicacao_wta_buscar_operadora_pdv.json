{"LAYOUTCOMUNICACAO":{
		"name": "WTA - Buscar Operadora - PDV",
		"request": {
			"method": "GET",
			"header": [
				{
					"key": "Authorization",
					"value": "Bearer {{TOKEN}}"
				},
				{
					"key": "Accept",
					"value": "*/*"
				}
			],
			"url": {
				"raw": "{{URL_BASE}}/winthor/venda/v0/operadoras",
				"query": [
					{
						"key": "tamanho",
						"value": "{{PAGE_SIZE}}"
					},
					{
						"key": "pagina",
						"value": "{{PAGE}}"
					} ,
	                {
						"key": "ultimaAlteracao",
						"value": "{{LAST_CHANGE}}"
					} ,
                    {
						"key": "tipoOperacao",
						"value": "01, 02"
					}
				]
			}
		},
		"response": []
	}}

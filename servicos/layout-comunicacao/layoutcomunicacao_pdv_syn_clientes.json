{"LAYOUTCOMUNICACAO":{
	"name": "PDV-SYNC Salvar Clientes",
	"request": {
		"method": "POST",
		"header": [
		    {"key": "Authorization", "value": "Bearer {{TOKEN}}"},
			{
				"key": "Accept",
				"value": "*/*"
			},
			{
				"key": "Content-Type",
				"value": "application/json"
			}  
		],
		"url": {
			"raw": "{{URL_ENVIAR_CLIENTE}}",
			"query": [
				{
					"key": "pageSize",
					"value": "{{PAGE_SIZE}}"
				},
				{
					"key": "page",
					"value": "{{PAGE}}"
				}
			]
		},
		"bodyraw": "{{CLIENT_JSON}}"
	},
	"response": []
}}

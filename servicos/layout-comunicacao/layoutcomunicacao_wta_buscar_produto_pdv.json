{"LAYOUTCOMUNICACAO":{
		"name": "WTA - Buscar Produto - PDV",
		"request": {
			"method": "GET",
			"header": [
				{
					"key": "Authorization",
					"value": "Bearer {{TOKEN}}"
				},
				{
					"key": "Accept",
					"value": "*/*"
				}
			],
			"url": {
				"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/produtotributacao/consultar",
				"query": [
					{
						"key": "pageSize",
						"value": "{{PAGE_SIZE}}"
					},
					{
						"key": "page",
						"value": "{{PAGE}}"
					} ,
	   			    {
						"key": "filial",
						"value": "{{FILIAL}}"
					} ,
	                {
						"key": "dataUltimaAlteracao",
						"value": "{{LAST_CHANGE}}"
					},
					{
						"key": "dataExclusao",
						"value": "{{DATAMENOS60DIAS}}"
					},
					{
						"key": "revenda",
						"value": "{{REVENDA}}"
					},
					{
						"key": "tipoMercadoria",
						"value": "{{TIPOMERCADORIA}}"
					}
				]
			}
		},
		"response": []
	}}

{"LAYOUTCOMUNICACAO":{
	"name": "WTA -Buscar produtos",
	"request": {
		"method": "GET",
		"header": [
			{
				"key": "Authorization",
				"value": "Bearer {{TOKEN}}"
			},
			{
				"key": "Accept",
				"value": "*/*"
			}
		],
		"url": {
			"raw": "{{URL_BASE}}/api/wholesale/v1/customer/list",
			"query": [
				{
					"key": "pageSize",
					"value": "{{PAGE_SIZE}}"
				},
				{
					"key": "page",
					"value": "{{PAGE}}"
				},
				{
					"key": "lastChange",
					"value": "{{DATAALTERACAO}}"
				}
			]
		}
	},
	"response": []
}}

{"LAYOUTCOMUNICACAO":{
	"name": "WTA -Buscar Imposto NCM",
	"request": {
		"method": "GET",
		"header": [
			{
				"key": "Authorization",
				"value": "Bearer {{TOKEN}}"
			},
			{
				"key": "Accept",
				"value": "*/*"
			}
		],
		"url": {
			"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/ncm/consultar",
			"query": [
				{
					"key": "pageSize",
					"value": "{{PAGE_SIZE}}"
				},
				{
					"key": "page",
					"value": "{{PAGE}}"
				} ,
        {
					"key": "dataUltimaALteracao",
					"value": "{{LAST_CHANGE}}"
				}
			]
		}
	},
	"response": []
}}

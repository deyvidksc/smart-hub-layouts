{"CHAVE":"{{LAST_CHANGE}}","CODEMPALTER":"","DTULTALTER":"","ID":"69","IDROTASERVICO":"40","TIPOCHAVE":"BODY","TIPOVALOR":"SELECT","VALOR":"SELECT
	(CASE 
		WHEN (SELECT COUNT(1) FROM PCINTEGRACAOCORE  WHERE IDROTASERVICO = 36 AND STATUS != 1 AND ROWNUM >= 1) > 0
		THEN TO_CHAR(NVL((SELECT MAX(DATACRIACAO) FROM PCINTEGRACAOCORE WHERE IDROTASERVICO = 40 AND STATUS = 2), TO_DATE('01/01/1900', 'DD/MM/YYYY')) -1, 'RRRR-MM-DD"T"HH24:MI:SS')
		ELSE 
			 TO_CHAR(SYSDATE, 'RRRR-MM-DD"T"HH24:MI:"00"')
	END) DT
FROM DUAL "}

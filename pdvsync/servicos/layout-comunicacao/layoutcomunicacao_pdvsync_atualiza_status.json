{"LAYOUTCOMUNICACAO":{
    "name": "PDVSync - atualizar status",
    "request": {
        "method": "PUT",
        "header": [
            {"key": "Authorization", "value": "Bearer {{TOKEN}}"},
            {
                "key": "Accept",
                "value": "*/*"
            },
            {
                "key": "Content-Type",
                "value": "application/json"
            }
        ],
        "url": {
            "raw": "{{URL_ATUALIZAR_STATUS_PEDIDO}}",
            "query": []
        },
        "bodyraw": [{ "idMensagem": "{{ID_MENSAGEM}}",  "status": "{{STATUS_INTEGRACAO}}" }]
    },
    "response": []
}}

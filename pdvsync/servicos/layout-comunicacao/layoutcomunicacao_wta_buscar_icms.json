{"LAYOUTCOMUNICACAO":{
	"name": "WTA -Buscar ICMS",
	"request": {
		"method": "GET",
		"header": [
			{
				"key": "Authorization",
				"value": "Bearer {{TOKEN}}"
			},
			{
				"key": "Accept",
				"value": "*/*"
			}
		],
		"url": {
			"raw": "{{URL_BASE}}/winthor/tributacao/v0/saida/icms/consultar",
			"query": [
				{
					"key": "tamanho",
					"value": "{{PAGE_SIZE}}"
				},
				{
					"key": "pagina",
					"value": "{{PAGE}}"
				},
				
				{
					"key": "dataUltimaAlteracao",
					"value": "{{LAST_CHANGE}}"
				}
			]
		}
	},
	"response": []
}}
